    <body class="bg-zinc-900">
            <Navbar/>
                <main class="bg-zinc-900 py-28 mx-[16%] lg:top-[12%] lg:gap-4 flex justify-center">
                    <slot/>
                </main>
            <Footer/>
    </body>
</html>